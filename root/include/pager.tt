    <div class="prevNext">
        [% prev = Ovid.prev_post(type, slug) %]
        <a href="/[% IF prev; type _ '/' _ prev.slug _ '.html';  ELSE; '#'; END %]" class="previous">[% IF prev %]&laquo; [% prev.title | html %][% END %]</a>
        [% next = Ovid.next_post(type, slug) %]
        <a href="/[% IF next; type _ '/' _ next.slug _ '.html'; ELSE; '#'; END %]" class="nextious">[% IF next %][% next.title | html %] &raquo;[% END %]</a>
    </div>
